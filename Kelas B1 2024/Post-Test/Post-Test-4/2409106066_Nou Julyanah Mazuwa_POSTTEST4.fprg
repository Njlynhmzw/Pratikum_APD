    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="usernamevalid, username" type="String" array="False" size=""/>
            <declare name="passwordvalid, password" type="String" array="False" size=""/>
            <declare name="kesempatan" type="Integer" array="False" size=""/>
            <assign variable="usernamevalid" expression="&quot;nou&quot;"/>
            <assign variable="passwordvalid" expression="&quot;066&quot;"/>
            <assign variable="kesempatan" expression="3"/>
            <while expression="kesempatan &gt; 0">
                <output expression="&quot;masukkan username&quot;" newline="True"/>
                <input variable="username"/>
                <output expression="&quot;masukkan password&quot;" newline="True"/>
                <input variable="password"/>
                <if expression="username == usernamevalid and password == passwordvalid">
                    <then>
                        <output expression="&quot;berhasil login&quot;" newline="True"/>
                        <declare name="menu" type="Integer" array="False" size=""/>
                        <declare name="panjang, lebar, tinggi, rusuk, luaskubus, kelilingbalok, volumebalok" type="Real" array="False" size=""/>
                        <output expression="&quot;=======================================================&quot; &amp; &#13;&#10;ToChar(13)&amp; &#13;&#10;&quot;menu program menghitung luas permukaan, keliling, dan volume berbagai bangun ruang&quot;&amp; &#13;&#10;ToChar(13)&amp;&#13;&#10;&quot;======================================================&quot;&amp;&#13;&#10;ToChar(13)&amp;&#13;&#10;&quot;1. hitunglah luas kubus&quot;&amp;&#13;&#10;ToChar(13)&amp;&#13;&#10;&quot;2. volume balok&quot;&amp;&#13;&#10;ToCHar(13)&amp;&#13;&#10;&quot;3. keliling balok&quot;&amp;&#13;&#10;ToChar(13)&amp;&#13;&#10;&quot;4. keluar dari program&quot;&amp;&#13;&#10;ToChar(13)&amp;&#13;&#10;&#13;&#10;&quot;======================================================&quot;" newline="True"/>
                        <output expression="&quot;Masukkan pilihan menu&quot;" newline="True"/>
                        <input variable="menu"/>
                        <if expression="menu==1">
                            <then>
                                <output expression="&quot;hitunglah luas kubus&quot;" newline="True"/>
                                <output expression="&quot;masukkan panjang rusuk (dalam cm):&quot;" newline="True"/>
                                <input variable="rusuk"/>
                                <assign variable="luaskubus" expression="6*rusuk*rusuk"/>
                                <output expression="&quot;luas kubus dengan rusuk&quot;&amp; rusuk &amp; &quot;cm&quot;&amp;  &quot;adalah&quot; &amp; &#13;&#10;luaskubus&amp; &quot;cm^2&quot;" newline="True"/>
                                <declare name="lagi, yes, no, continue, break, exit" type="String" array="False" size=""/>
                                <output expression="&quot;kembali ke program awal untuk memilih menu? (yes/no)&quot;" newline="True"/>
                                <input variable="lagi"/>
                                <if expression="lagi == &quot;yes&quot;">
                                    <then>
                                        <assign variable="continue" expression="&quot;kembali ke program awal&quot;"/>
                                    </then>
                                    <else>
                                        <if expression="lagi == &quot;no&quot;">
                                            <then>
                                                <output expression="&quot;keluar program&quot;" newline="True"/>
                                                <output expression="&quot;==========================================================&quot;" newline="True"/>
                                                <breakpoint expression="" always="True"/>
                                            </then>
                                            <else>
                                                <output expression="&quot;pilihan tidak valid&quot;" newline="True"/>
                                                <breakpoint expression="" always="True"/>
                                            </else>
                                        </if>
                                    </else>
                                </if>
                            </then>
                            <else>
                                <if expression="menu==2">
                                    <then>
                                        <output expression="&quot;hitunglah volume balok&quot;" newline="True"/>
                                        <output expression="&quot;masukkan panjang(dalam cm):&quot;" newline="True"/>
                                        <input variable="panjang"/>
                                        <output expression="&quot;masukkan lebar(dalam cm):&quot;" newline="True"/>
                                        <input variable="lebar"/>
                                        <output expression="&quot;masukkan tinggi(dalam cm):&quot;" newline="True"/>
                                        <input variable="tinggi"/>
                                        <assign variable="Volumebalok" expression="panjang*lebar*tinggi"/>
                                        <output expression="&quot;volume balok dengan &quot;&amp; &quot;panjang&quot;&amp; panjang &amp; &quot;cm&quot;&amp; &quot;lebar&quot;&amp; lebar &amp; &quot;cm&quot;&amp; &quot;tinggi&quot;&amp; tinggi &amp; &quot;cm&quot;&amp; &quot;adalah&quot; &amp; &#13;&#10;volumebalok &amp; &quot;cm^3&quot;" newline="True"/>
                                        <declare name="lagi, yes, no, continue, break, exit" type="String" array="False" size=""/>
                                        <output expression="&quot;kembali ke program awal untuk memilih menu? (yes/no)&quot;" newline="True"/>
                                        <input variable="lagi"/>
                                        <if expression="lagi == &quot;yes&quot;">
                                            <then>
                                                <assign variable="continue" expression="&quot;kembali ke program awal&quot;"/>
                                            </then>
                                            <else>
                                                <if expression="lagi == &quot;no&quot;">
                                                    <then>
                                                        <output expression="&quot;keluar program&quot;" newline="True"/>
                                                        <output expression="&quot;==========================================================&quot;" newline="True"/>
                                                        <breakpoint expression="" always="True"/>
                                                    </then>
                                                    <else>
                                                        <output expression="&quot;pilihan tidak valid&quot;" newline="True"/>
                                                        <breakpoint expression="" always="True"/>
                                                    </else>
                                                </if>
                                            </else>
                                        </if>
                                    </then>
                                    <else>
                                        <if expression="menu==3">
                                            <then>
                                                <output expression="&quot;hitunglah keliling balok&quot;" newline="True"/>
                                                <output expression="&quot;masukkan panjang(dalam cm):&quot;" newline="True"/>
                                                <input variable="panjang"/>
                                                <output expression="&quot;masukkan lebar(dalam cm):&quot;" newline="True"/>
                                                <input variable="lebar"/>
                                                <output expression="&quot;masukkan tinggi(dalam cm):&quot;" newline="True"/>
                                                <input variable="tinggi"/>
                                                <assign variable="Kelilingbalok" expression="4*(panjang+lebar+tinggi)"/>
                                                <output expression="&quot;keliling balok dengan &quot;&amp; &quot;panjang&quot;&amp; panjang &amp; &quot;cm&quot;&amp; &quot;lebar&quot;&amp; lebar &amp; &quot;cm&quot;&amp; &quot;tinggi&quot;&amp; tinggi &amp; &quot;cm&quot;&amp; &quot;adalah&quot; &amp; &#13;&#10;kelilingbalok &amp; &quot;cm&quot;" newline="True"/>
                                                <declare name="lagi, yes, no, continue, break, exit" type="String" array="False" size=""/>
                                                <output expression="&quot;kembali ke program awal untuk memilih menu? (yes/no)&quot;" newline="True"/>
                                                <input variable="lagi"/>
                                                <if expression="lagi == &quot;yes&quot;">
                                                    <then>
                                                        <assign variable="continue" expression="&quot;kembali ke program awal&quot;"/>
                                                    </then>
                                                    <else>
                                                        <if expression="lagi == &quot;no&quot;">
                                                            <then>
                                                                <output expression="&quot;keluar program&quot;" newline="True"/>
                                                                <output expression="&quot;==========================================================&quot;" newline="True"/>
                                                                <breakpoint expression="" always="True"/>
                                                            </then>
                                                            <else>
                                                                <output expression="&quot;pilihan tidak valid&quot;" newline="True"/>
                                                                <breakpoint expression="" always="True"/>
                                                            </else>
                                                        </if>
                                                    </else>
                                                </if>
                                            </then>
                                            <else>
                                                <output expression="&quot;keluar dari program&quot;" newline="True"/>
                                                <breakpoint expression="" always="True"/>
                                            </else>
                                        </if>
                                    </else>
                                </if>
                            </else>
                        </if>
                    </then>
                    <else>
                        <output expression="&quot;username atau password salah !&quot;" newline="True"/>
                        <assign variable="kesempatan" expression="kesempatan - 1"/>
                        <output expression="&quot;kesempatan tersisa&quot; &amp; kesempatan &amp; &quot;kali&quot;" newline="True"/>
                        <output expression="&quot;login gagal&quot;" newline="True"/>
                    </else>
                </if>
            </while>
        </body>
    </function>
